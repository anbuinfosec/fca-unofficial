  "directories": {
    "test": "test",
    "lib": "lib",
    "examples": "examples"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "utils.js",
    "src/",
    "lib/",
    "README.md",
    "DOCS.md",
    "LICENSE-MIT"
